{
	"name": "svelte-qrcode-image",
	"version": "1.1.0",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"publish": "svelte-kit sync && svelte-package && npm publish ./package"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "2.4.3",
		"@sveltejs/package": "2.3.7",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/qrcode": "^1.5.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.0",
		"tslib": "^2.3.1",
		"typescript": "^5.5.0",
		"vite": "^5.4.4"
	},
	"type": "module",
	"dependencies": {
		"qrcode": "^1.5.1"
	},
	"homepage": "https://svelte-qrcode-image.itoldyou.dev/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/1toldyou/svelte-qrcode-image.git"
	},
	"bugs": {
		"url": "https://github.com/1toldyou/svelte-qrcode-image/issues"
	},
	"keywords": [
		"qrcode",
		"qr-code",
		"qr",
		"svelte",
		"sveltekit",
		"svelte-kit"
	],
	"license": "MIT"
}
